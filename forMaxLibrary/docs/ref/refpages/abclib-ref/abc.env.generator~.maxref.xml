    <outlet id="0" type="OUTLET_TYPE">
      <digest></digest>
      <description></description>
    </outlet>
  </outletlist> -->

  <objarglist>
      <objarg name="grain-size" optional="0" type="float">
          <digest>
              the grain size (ms)
          </digest>
          <description>
              [ONLY IN INSPECTOR] first argument is the grain size (envelop period in milliseconds)
          </description>
      </objarg>
      <objarg name="rarefaction" optional="0" type="float">
          <digest>
            rarefaction
          </digest>
          <description>
            [ONLY IN INSPECTOR] second argument is rarefaction between 0 and 1
          </description>
      </objarg>
  </objarglist>

