<c74object name="abc.env.noise~" module="abclib">
  <digest>Abclib's enveloped noise with rarefaction </digest>
  <description><o>abc.env.noise~</o> generates noise enveloped with a positive cosine shape (Hanning window). It uses rarefaction.
  <br /><br /></description>

  <!--INLETS-->
  <!-- <inletlist>
    <inlet id="0" type="INLET_TYPE">
      <digest></digest>
      <description></description>
    </inlet>
  </inletlist> -->

  <!--OUTLETS-->
  <!-- <outletlist>
    <outlet id="0" type="OUTLET_TYPE">
      <digest></digest>
      <description></description>
    </outlet>
  </outletlist> -->

  <objarglist>
  </objarglist>

  <!--ATTRIBUTES-->
  <attributelist>
    <attribute name="grainsize" type="float" set="1" get="1" size="1">
      <digest>the grain size (ms)</digest>
      <description>[ONLY IN INSPECTOR] sets the initial grain size (envelop period in milliseconds)</description>
    </attribute>

    <attribute name="rarefaction" type="float" set="1" get="1" size="1">
          <digest>rarefaction</digest>
          <description>[ONLY IN INSPECTOR] sets the initial rarefaction between 0 and 1</description>
    </attribute>

    <attribute name="gain" type="float" set="1" get="1" size="1">
      <digest>gain</digest>
      <description>[ONLY IN INSPECTOR] sets the initial gain in dB</description>
    </attribute>
  </attributelist>

  <!--MESSAGES-->
  <methodlist>  
  </methodlist>

  <seealsolist>
    <seealso name="abc.mc.multinoise~" />
    <seealso name="abc.puckettespaf~" />
    <seealso name="abc.rissetsbell~" />
  </seealsolist>
</c74object>
